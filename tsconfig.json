{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true, // added this line
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  // "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts",
    ".next/types/**/*.ts"
, "pages/dashboard.tsx", "pages/**/*.tsx", "pages/form.tsx", "pages/index.tsx", "pages/login.tsx", "pages/signup.tsx", "src/app/layout.tsx", "src/app/api/submitEmail/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
